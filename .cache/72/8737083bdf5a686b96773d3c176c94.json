width: 100%;\n    background-color: $light-gray;\n    text-align: center;\n    padding: 50px 20px;\n    margin: 0 30px;\n    border-radius: 30px;\n}\n\n.title-text {\n    text-transform: uppercase;\n    font-weight: $extra-bold-weight;\n    font-size: clamp(1.75rem, 2.5vw, 3rem);\n}\n\n.buttons {\n    margin: 30px auto;\n    border: 1px solid #000000;\n    border-radius: 30px;\n    p {\n        text-transform: uppercase;\n        font-size: clamp(0.4rem, 2.5vw, 0.75rem);\n        padding: 10px;\n    }\n}\n\n.active-button {\n    color: $white;\n    font-weight: $bold-weight;\n    background-color: #5C5470;\n    border-radius: 30px;\n}\n\n.personal-projects, .frontendmentor {\n    flex-wrap: wrap;\n    align-items: center;\n    justify-content: center;\n}\n\n.frontendmentor {\n    display: none !important;\n}\n\n.card {\n    text-align: left;\n    width: 300px;\n    margin: 30px 0;\n    background-color: $cards-color;\n    border: 0;\n    border-radius: 0 20px 20px 20px;\n    padding: 15px;\n    position: relative;\n    &::before {\n        content: url('../images/card-top.svg');\n        position: absolute;\n        top: -29px;\n        left: 0;\n    }\n}\n\n.card-title {\n    font-weight: $bold-weight;\n    font-size: clamp(1.125rem, 2.5vw, 1.375rem);\n}\n.card-subtitle {\n    text-transform: uppercase;\n    font-size: clamp(0.688rem, 2.5vw, 0.75rem);\n    opacity: 0.8;\n}\n.card-text {\n    h4, p {\n        opacity: 1;\n        @extend .card-subtitle;\n    }\n    h4 {\n        font-weight: $bold-weight;\n    }\n}\n\n.links {\n    display: flex;\n    justify-content: center;\n}\n\n.card-link {\n    color: $black;\n    font-weight: $bold-weight;\n    text-decoration: none;\n    border-bottom: 2px solid #000;\n    margin: 0 20px;\n    @extend .card-subtitle;\n}\n\n@media  screen and (min-width: 900px) {\n    section {\n        align-items: unset;\n        justify-content: unset;\n    }\n    #sidebar {\n        width: 500px;\n        align-items: center;\n        justify-content: center;\n        padding: 0 50px;\n        position: relative;\n    }\n\n    .list-of-links {\n        align-items: center;\n        justify-content: center;\n        margin: 50px 0;\n        a {\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            width: 25rem;\n        }\n        span {\n            display: block;\n            margin-left: 15px;\n            font-weight: $bold-weight;\n            font-size: 1.25rem;\n        }\n    }\n\n    footer {\n        position: absolute;\n        bottom: 0;\n    }\n\n    #projects {\n        width: 100%;\n        margin: 0 30px 0 0;\n        padding: 80px 0;\n    }\n\n    .list-of-projects {\n        margin: 30px auto;\n    }\n\n    .card {\n        margin: 30px;\n    }\n}","styles/_variables.scss":"// Font Styles\n$family: 'Inter', sans-serif;\n$body-font-size: 1rem;\n$regular-weight: 400;\n$medium-weight: 500;\n$semi-bold-weight: 600;\n$bold-weight: 700;\n$extra-bold-weight: 800;\n$header-title: clamp(1.75rem, 2.5vw, 3rem); // font sizes for Project, About and Contact\n\n// Colors\n$dark-color: hsl(257, 18%, 23%);\n$light-gray: hsl(0, 8%, 95%);\n$cards-color: hsl(256, 16%, 87%);\n$black: hsl(0, 0%, 0%);\n$white: hsl(0, 0%, 100%);\n\n\n// Font-sizes\n// $name-size: clamp(1.25rem, 2.5vw, 2rem);\n// $description-size: clamp(0.75rem, 2.5vw, 1.25rem);\n// \n\n"},"lineCount":null}},"error":null,"hash":"35f8f577f27d1c6bd3c9a2f357380aa3","cacheData":{"env":{}}}